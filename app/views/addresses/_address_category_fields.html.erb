<div id="nested-fields" class="row nested-fields address-category-fields">
  <div class=" category_from_list">
    <div class="col-xs-10">
      <%= f.association :category, :collection => Category.all.order(:name), :prompt => 'Choose an existing category', :label_html => {class: 'login-label'},:input_html => {class: 'transparent-input country-select'} %>
    </div>
    <div class="col-xs-2 center-align ">
      <%= link_to_remove_association "X", f, class: 'btn btn-danger btn-remove-category' %>

    </div>
  </div>
  <%# link_to_add_association 'or create a new tag', f, :category %>
</div>